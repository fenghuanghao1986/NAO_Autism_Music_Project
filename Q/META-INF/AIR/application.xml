<?xml version="1.0" encoding="utf-8" standalone="no"?>
<application xmlns="http://ns.adobe.com/air/application/3.1">
<!-- application xmlns="http://ns.adobe.com/air/application/1.5" -->

<!-- Adobe AIR Application Descriptor File Template.

	Specifies parameters for identifying, installing, and launching AIR applications.

	xmlns - The Adobe AIR namespace: http://ns.adobe.com/air/application/1.5
			The last segment of the namespace specifies the version 
			of the AIR runtime required for this application to run.
			
	minimumPatchLevel - The minimum patch level of the AIR runtime required to run 
			the application. Optional.
-->

	<!-- The application identifier string, unique to this application. Required. -->
	<id>Q</id>

	<!-- Used as the filename for the application. Required. -->
	<filename>Q</filename>

	<!-- The name that is displayed in the AIR application installer. 
	     May have multiple values for each language. See samples or xsd schema file. Optional. -->
	<name/>

	<!-- An application version designator (such as "v1", "2.5", or "Alpha 1"). Required. -->
	<versionNumber>2.01.56</versionNumber>

	<!-- Description, displayed in the AIR application installer.
	     May have multiple values for each language. See samples or xsd schema file. Optional. -->
	<description>Affectiva Q version 2.01.56 compiled at 20130401.1523</description>

	<!-- Copyright information. Optional -->
	<copyright>2009-2011 Affectiva Inc. All rights reserved.</copyright>

	<!-- Settings for the application's initial window. Required. -->
	<initialWindow>
		<!-- The main SWF or HTML file of the application.buttonAlignment Required. -->
		<!-- Note: In Flex Builder, the SWF reference is set automatically. -->
		<content>Q.swf</content>
		
		<!-- The title of the main window. Optional. -->
		<title>Q</title>

		<!-- The type of system chrome to use (either "standard" or "none"). Optional. Default standard. -->
		<systemChrome>standard</systemChrome>

		<!-- Whether the window is transparent. Only applicable when systemChrome is none. Optional. Default false. -->
		<transparent>false</transparent>

		<!-- Whether the window is initially visible. Optional. Default false. -->
		<!-- <visible></visible> -->

		<!-- Whether the user can minimize the window. Optional. Default true. -->
		<!-- <minimizable></minimizable> -->

		<!-- Whether the user can maximize the window. Optional. Default true. -->
		<!-- <maximizable></maximizable> -->

		<!-- Whether the user can resize the window. Optional. Default true. -->
		<!-- <resizable></resizable> -->

		<!-- The window's initial width. Optional. -->
		<!-- <width></width> -->

		<!-- The window's initial height. Optional. -->
		<!-- <height></height> -->

		<!-- The window's initial x position. Optional. -->
		<!-- <x></x> -->

		<!-- The window's initial y position. Optional. -->
		<!-- <y></y> -->

		<!-- The window's minimum size, specified as a width/height pair, such as "400 200". Optional. -->
		<minSize>400 320</minSize>

		<!-- The window's initial maximum size, specified as a width/height pair, such as "1600 1200". Optional. -->
		<!-- <maxSize></maxSize> -->
	</initialWindow>

	<!-- The subpath of the standard default installation location to use. Optional. -->
	<!-- <installFolder></installFolder> -->

	<!-- The subpath of the Programs menu to use. (Ignored on operating systems without a Programs menu.) Optional. -->
	<!-- <programMenuFolder></programMenuFolder> -->

	<!-- The icon the system uses for the application. For at least one resolution,
		 specify the path to a PNG file included in the AIR package. Optional. -->
	<icon>
		<image16x16>assets/icons/16.png</image16x16>
		<image32x32>assets/icons/32.png</image32x32>
		<image48x48>assets/icons/48.png</image48x48>
		<image128x128>assets/icons/128.png</image128x128>
	</icon>

	<!-- Whether the application handles the update when a user double-clicks an update version
	of the AIR file (true), or the default AIR application installer handles the update (false).
	Optional. Default false. -->
	<!-- <customUpdateUI></customUpdateUI> -->
	
	<!-- Whether the application can be launched when the user clicks a link in a web browser.
	Optional. Default false. -->
	<!-- <allowBrowserInvocation></allowBrowserInvocation> -->

	<!-- Listing of file types for which the application can register. Optional. -->
	<fileTypes>

		<fileType>
			<name>EDA</name>
			<extension>eda</extension>
			<description>Affectiva EDA File</description>
			<contentType>text/plain</contentType>

			<icon>
				<image16x16>assets/fileTypeIcons/16.png</image16x16>
				<image32x32>assets/fileTypeIcons/32.png</image32x32>
				<image48x48>assets/fileTypeIcons/48.png</image48x48>
				<image128x128>assets/fileTypeIcons/128.png</image128x128>
			</icon>
			
		</fileType>
		
		<fileType>
			<name>QPRO</name>
			<extension>qpro</extension>
			<description>Affectiva Q Project File</description>
			<contentType>application/zip</contentType>

			<icon>
				<image16x16>assets/fileTypeIcons/qpro16.png</image16x16>
				<image32x32>assets/fileTypeIcons/qpro32.png</image32x32>
				<image48x48>assets/fileTypeIcons/qpro48.png</image48x48>
				<image128x128>assets/fileTypeIcons/qpro128.png</image128x128>
			</icon>
			
		</fileType>
		
	</fileTypes>
	
	<supportedProfiles>extendedDesktop desktop</supportedProfiles>

</application>
